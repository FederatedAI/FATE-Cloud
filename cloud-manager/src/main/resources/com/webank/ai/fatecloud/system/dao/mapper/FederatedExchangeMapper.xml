<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webank.ai.fatecloud.system.dao.mapper.FederatedExchangeMapper">

    <select id="findExchangePage"
            parameterType="com.webank.ai.fatecloud.system.pojo.qo.ExchangePageQo" resultType="com.webank.ai.fatecloud.system.dao.entity.FederatedExchangeDo">
        select * FROM t_federated_exchange order by exchange_name asc limit #{startIndex}, #{exchangePageQo.pageSize}
    </select>

</mapper>
